<%= form_for inquiry do |f| %>
  <% if inquiry.errors.any? %>
   
      <% inquiry.errors.full_messages.each do |msg| %>
        <%= msg %>
      <% end %>
    
  <% end %>
  
 
    <%= f.label :nickname %><br/>
    <%= f.text_field :nickname %>

  
 
    <%= f.label :name %><br/>
    <%= f.text_field :name %>

 
    <%= f.label :email %><br/>
    <%= f.text_field :email %>
 
  
  
    <%= f.label :message %><br/>
    <%= f.text_area :message %>
  
  
<%= f.submit "Send", class: "btn btn-large btn-primary"%>
  
 
<% end %>